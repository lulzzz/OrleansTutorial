    "SiloConfig": {
      "ClusterId": "dev",
      "ServiceId": "HelloWorldApp",
      "SiloPort": 10000,
      "GatewayPort": 30000
    },
    "Provider": {
      "DefaultProvider": "MongoDB",
      "MongoDB": {
        "Cluster": {
          "DbConn": "mongodb://localhost:27017",
          "DbName": "Orleans-Clustering",
          "CollectionPrefix": "netcore-demo"
        },
        "Storage": {
          "DbConn": "mongodb://localhost:27017",
          "DbName": "Orleans-Storage"
        },
        "Reminder": {
          "DbConn": "mongodb://localhost:27017",
          "DbName": "Orleans-Reminder"
        }
      }
    },
    "Dashboard": {
      "enable": true,
      "port": 8099
    }
  }
}
